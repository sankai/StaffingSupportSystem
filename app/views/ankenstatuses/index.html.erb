<h1>Listing ankenstatuses</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @ankenstatuses.each do |ankenstatus| %>
  <tr>
    <td><%= ankenstatus.name %></td>
    <td><%= link_to '表示', ankenstatus %></td>
    <td><%= link_to '編集', edit_ankenstatus_path(ankenstatus) %></td>
    <td><%= link_to '削除', ankenstatus, method: :delete, data: { confirm: '削除してよろしいですか？' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Ankenstatus', new_ankenstatus_path %>
