<h1>Listing ankenstatuses</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @ankenstatuses.each do |ankenstatus| %>
  <tr>
    <td><%= ankenstatus.name %></td>
    <td><%= link_to 'Show', ankenstatus %></td>
    <td><%= link_to 'Edit', edit_ankenstatus_path(ankenstatus) %></td>
    <td><%= link_to 'Destroy', ankenstatus, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

